{
  "name": "protractor",
  "dependencies": {
    "protractor": "^5.4.1",
    "protractor-beautiful-reporter": "^1.2.5",
    "typescript": "^3.1.4",
    "tslint": "5.11.0",
    "jasmine": "^3.3.0",
    "@types/jasmine": "^2.8.9",
    "@types/jasminewd2": "^2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/angular/protractor.git"
  },
  "scripts": {
    "setup": "npm install && node node_modules/protractor/bin/webdriver-manager update --gecko=false",
    "webdriverStart": "./node_modules/.bin/webdriver-manager start",
    "tsc": "tsc",
    "pretest": "npm run tsc",
    "test": "./node_modules/.bin/protractor tmp/run_config.js",
    "lint": "./node_modules/.bin/tslint --force -c tslint.json ./core/**/*.ts",
    "lint-fix": "./node_modules/.bin/tslint --fix ./specs/**/*.ts -t verbose"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "jasmine-spec-reporter": "^4.2.1",
    "tslint-eslint-rules": "^5.4.0"
  }
}
